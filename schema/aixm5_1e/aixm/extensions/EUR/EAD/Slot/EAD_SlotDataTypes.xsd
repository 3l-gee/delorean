<!-- Component: EAD: Slot: DataTypes -->
<schema xmlns:EAD_Slot="http://www.aixm.aero/schema/5.1/extensions/EUR/EAD/Slot" xmlns:aixm="http://www.aixm.aero/schema/5.1" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" targetNamespace="http://www.aixm.aero/schema/5.1/extensions/EUR/EAD/Slot" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<import namespace="http://www.aixm.aero/schema/5.1" schemaLocation="../../../../AIXM_DataTypes.xsd"/>
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../../../ISO_19136_Schemas/gml.xsd"/>
	<annotation>
		<appinfo>
			<gml:gmlProfileSchema>http://www.aixm.aero/schema/5.1/profile/gml4aixm.xsd</gml:gmlProfileSchema>
		</appinfo>
	</annotation>
	<simpleType name="CodeSlotSourceBaseType">
		<annotation>
			<appinfo>
				<gml:description>The source of the private slot.</gml:description>
			</appinfo>
		</annotation>
		<union>
			<simpleType>
				<restriction base="xsd:string">
					<enumeration value="AIRAC_AMDT">
						<annotation>
							<appinfo>
								<gml:description>AIRAC AIP Amendment.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="AMDT">
						<annotation>
							<appinfo>
								<gml:description>Non-AIRAC AIP Amendment.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="AIRAC_SUP">
						<annotation>
							<appinfo>
								<gml:description>AIRAC AIP Supplement.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="SUP">
						<annotation>
							<appinfo>
								<gml:description>Non-AIRAC AIP Supplement.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="AIC">
						<annotation>
							<appinfo>
								<gml:description>Aeronautical Information Circular.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="NOTAM">
						<annotation>
							<appinfo>
								<gml:description>NOTAM Message.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="UPDATE">
						<annotation>
							<appinfo>
								<gml:description>Database update (no source).</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
				</restriction>
			</simpleType>
			<simpleType>
				<restriction base="string">
					<pattern value="OTHER(:(\w|_){1,58})?"/>
				</restriction>
			</simpleType>
		</union>
	</simpleType>
	<complexType name="CodeSlotSourceType">
		<simpleContent>
			<extension base="EAD_Slot:CodeSlotSourceBaseType">
				<attribute name="nilReason" type="gml:NilReasonEnumeration"/>
			</extension>
		</simpleContent>
	</complexType>
	<simpleType name="CodePublicSlotStatusBaseType">
		<annotation>
			<appinfo>
				<gml:description>The status indicator of the public slot.</gml:description>
			</appinfo>
		</annotation>
		<union>
			<simpleType>
				<restriction base="xsd:string">
					<enumeration value="CONSISTENT">
						<annotation>
							<appinfo>
								<gml:description>After successful public slot checking.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="COMMITTED">
						<annotation>
							<appinfo>
								<gml:description>After successful committing of the Public Slot.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="INCONSISTENT">
						<annotation>
							<appinfo>
								<gml:description>After unsuccessful public slot checking.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="UNKNOWN">
						<annotation>
							<appinfo>
								<gml:description>After adding, modifying, removing private slots.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="DELETED">
						<annotation>
							<appinfo>
								<gml:description>After deletion.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="EXPIRED">
						<annotation>
							<appinfo>
								<gml:description>When the planned commit date is reached and the status is not Committed.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="EMTPY">
						<annotation>
							<appinfo>
								<gml:description>After its creation.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
				</restriction>
			</simpleType>
			<simpleType>
				<restriction base="string">
					<pattern value="OTHER(:(\w|_){1,58})?"/>
				</restriction>
			</simpleType>
		</union>
	</simpleType>
	<simpleType name="CodePrivateSlotStatusBaseType">
		<annotation>
			<appinfo>
				<gml:description>The status of the private slot.</gml:description>
			</appinfo>
		</annotation>
		<union>
			<simpleType>
				<restriction base="xsd:string">
					<enumeration value="CONSISTENT">
						<annotation>
							<appinfo>
								<gml:description>After successful private slot checking.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="COMMITTED">
						<annotation>
							<appinfo>
								<gml:description>After successful committing of the Public Slot.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="INCONSISTENT">
						<annotation>
							<appinfo>
								<gml:description>After unsuccessful private slot checking.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="CLOSED">
						<annotation>
							<appinfo>
								<gml:description>After successful closing of the private slot for static data element changes.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="UNKNOWN">
						<annotation>
							<appinfo>
								<gml:description>After adding, modifying, removing static data element changes.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="EXPIRED">
						<annotation>
							<appinfo>
								<gml:description>When the planned commit date is reached and the status is not Committed.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="EMTPY">
						<annotation>
							<appinfo>
								<gml:description>After its creation.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
				</restriction>
			</simpleType>
			<simpleType>
				<restriction base="string">
					<pattern value="OTHER(:(\w|_){1,58})?"/>
				</restriction>
			</simpleType>
		</union>
	</simpleType>
	<complexType name="CodePublicSlotStatusType">
		<simpleContent>
			<extension base="EAD_Slot:CodePublicSlotStatusBaseType">
				<attribute name="nilReason" type="gml:NilReasonEnumeration"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="CodePrivateSlotStatusType">
		<simpleContent>
			<extension base="EAD_Slot:CodePrivateSlotStatusBaseType">
				<attribute name="nilReason" type="gml:NilReasonEnumeration"/>
			</extension>
		</simpleContent>
	</complexType>
</schema>
