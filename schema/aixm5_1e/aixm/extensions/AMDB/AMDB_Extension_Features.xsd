<!-- Component: AIXM: AMDB Extension: Features -->
<schema xmlns:amdb="http://www.aixm.aero/schema/5.1/amdb" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:aixm="http://www.aixm.aero/schema/5.1" targetNamespace="http://www.aixm.aero/schema/5.1/amdb" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.1a">
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../ISO_19136_Schemas/gml.xsd"/>
	<import namespace="http://www.w3.org/1999/xlink" schemaLocation="../../xlink/xlinks.xsd"/>
	<import namespace="http://www.aixm.aero/schema/5.1" schemaLocation="../../AIXM_Features.xsd"/>
	<annotation>
		<appinfo>
			<gml:gmlProfileSchema>http://www.aixm.aero/schema/5.0/profile/gml4aixm.xsd</gml:gmlProfileSchema>
		</appinfo>
	</annotation>
	<annotation>
		<appinfo source="www.aixm.aero/schema/5.1">AIXM_Features.xsd</appinfo>
	</annotation>
	<element name="ApronElementExtension" type="amdb:ApronElementExtensionType" substitutionGroup="aixm:AbstractApronElementExtension"/>
	<complexType name="ApronElementExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:ApronElementExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="ApronElementExtensionPropertyGroup">
		<sequence>
			<element name="associatedTerminal" type="aixm:VerticalStructurePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="RunwayMarkingExtension" type="amdb:RunwayMarkingExtensionType" substitutionGroup="aixm:AbstractRunwayMarkingExtension"/>
	<complexType name="RunwayMarkingExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:RunwayMarkingExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="RunwayMarkingExtensionPropertyGroup">
		<sequence>
			<element name="protectedRunway" type="aixm:RunwayPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
			<element name="protectedTaxiway" type="aixm:TaxiwayPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
			<element name="landingDirection" type="aixm:RunwayDirectionPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="GroundTrafficControlServiceExtension" type="amdb:GroundTrafficControlServiceExtensionType" substitutionGroup="aixm:AbstractGroundTrafficControlServiceExtension"/>
	<complexType name="GroundTrafficControlServiceExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:GroundTrafficControlServiceExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="GroundTrafficControlServiceExtensionPropertyGroup">
		<sequence>
			<element name="extent" type="aixm:SurfacePropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The part of the airport surface where the service is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="ElevatedSurfaceExtension" type="amdb:ElevatedSurfaceExtensionType" substitutionGroup="aixm:AbstractElevatedSurfaceExtension"/>
	<complexType name="ElevatedSurfaceExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:ElevatedSurfaceExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="ElevatedSurfaceExtensionPropertyGroup">
		<sequence>
			<element name="horizontalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the horizontal resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="verticalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the vertical resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="ElevatedCurveExtension" type="amdb:ElevatedCurveExtensionType" substitutionGroup="aixm:AbstractElevatedCurveExtension"/>
	<complexType name="ElevatedCurveExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:ElevatedCurveExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="ElevatedCurveExtensionPropertyGroup">
		<sequence>
			<element name="horizontalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the horizontal resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="verticalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the vertical resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="ElevatedPointExtension" type="amdb:ElevatedPointExtensionType" substitutionGroup="aixm:AbstractElevatedPointExtension"/>
	<complexType name="ElevatedPointExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:ElevatedPointExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="ElevatedPointExtensionPropertyGroup">
		<sequence>
			<element name="horizontalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the horizontal resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="verticalResolution" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the vertical resolution (number of significant digits) with which the data is provided.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="AircraftStandExtension" type="amdb:AircraftStandExtensionType" substitutionGroup="aixm:AbstractAircraftStandExtension"/>
	<complexType name="AircraftStandExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="amdb:AircraftStandExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="AircraftStandExtensionPropertyGroup">
		<sequence>
			<element name="associatedTerminal" type="aixm:VerticalStructurePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
</schema>
