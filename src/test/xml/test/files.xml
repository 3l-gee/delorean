<?xml version="1.0" encoding="UTF-8"?>
<FeatureCollection xmlns="http://www.test.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <!-- Feature with Full Values -->
    <Feature>
        <a uom="KM">100</a>
    </Feature>

    <!-- Feature with Some Missing Attribute -->
    <Feature>
        <a>100</a>
    </Feature>

    <!-- Feature with Explicit Null Values (using xsi:nil) -->
    <Feature>
        <a xsi:nil="true"/>
    </Feature>

    <!-- Feature with NilReason Attribute -->
    <Feature>
        <a xsi:nil="true" nilReason="unknown"/>
    </Feature>

</FeatureCollection>
