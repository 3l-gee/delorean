<!-- Component: SimpleMetadata: DataTypes -->
<schema xmlns:metadata="http://www.aixm.aero/schema/5.1/extensions/SimpleMetadata" xmlns:aixm="http://www.aixm.aero/schema/5.1" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" targetNamespace="http://www.aixm.aero/schema/5.1/extensions/SimpleMetadata" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.1-a">
	<import namespace="http://www.aixm.aero/schema/5.1" schemaLocation="../../AIXM_DataTypes.xsd"/>
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../ISO_19136_Schemas/gml.xsd"/>
	<annotation>
		<appinfo>
			<gml:gmlProfileSchema>http://www.aixm.aero/schema/5.1/profile/gml4aixm.xsd</gml:gmlProfileSchema>
		</appinfo>
	</annotation>
	<annotation>
		<appinfo source="www.aixm.aero/schema/5.1/SimpleMetedata">AIXM_SimpleMetadata_DataTypes.xsd</appinfo>
	</annotation>
	<annotation>
		<documentation>Definition of data types used by a simple Metadata schema</documentation>
	</annotation>
	<simpleType name="ValDataIntegrityType">
		<annotation>
			<appinfo>
				<gml:description>Integrity of the data in the aeronautical data processing chain from data origination process to present data manipulation process.</gml:description>
			</appinfo>
		</annotation>
		<restriction base="xsd:float">
			<minInclusive value="0"/>
			<maxInclusive value="1"/>
		</restriction>
	</simpleType>
</schema>
