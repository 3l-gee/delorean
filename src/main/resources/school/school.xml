<?xml version="1.0" encoding="UTF-8"?>
<city xmlns="http://www.example.com/city" xmlns:gml="http://www.opengis.net/gml/3.2" name="SampleCity">

    <!-- List of houses -->
    <houses>
        <gml:SurfaceType housename="House1">
            <gml:patches>
                <gml:PolygonPatch>
                    <gml:exterior>
                        <gml:LinearRing>
                            <gml:posList>0 0 0 10 10 10 10 0 0 0</gml:posList>
                        </gml:LinearRing>
                    </gml:exterior>
                </gml:PolygonPatch>
            </gml:patches>
        </gml:SurfaceType>

        <gml:SurfaceType housename="House2">
            <gml:patches>
                <gml:PolygonPatch>
                    <gml:exterior>
                        <gml:LinearRing>
                            <gml:posList>20 20 20 30 30 30 30 20 20 20</gml:posList>
                        </gml:LinearRing>
                    </gml:exterior>
                </gml:PolygonPatch>
            </gml:patches>
        </gml:SurfaceType>
    </houses>

    <!-- List of streets -->
    <streets>
        <gml:LineStringType streetname="Street1">
            <gml:posList>0 0 10 10</gml:posList>
        </gml:LineStringType>

        <gml:LineStringType streetname="Street2">
            <gml:posList>10 10 20 20</gml:posList>
        </gml:LineStringType>
    </streets>

    <!-- List of poles -->
    <poles>
        <gml:PointType polename="Pole1">
            <gml:pos>5 5</gml:pos>
        </gml:PointType>

        <gml:PointType polename="Pole2">
            <gml:pos>15 15</gml:pos>
        </gml:PointType>
    </poles>

</city>
