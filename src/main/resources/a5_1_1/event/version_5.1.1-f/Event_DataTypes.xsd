<!-- Component: AIXM: Event Data Types -->
<schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.aixm.aero/schema/5.1.1/event"
    version="5.1.1.f" xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:aixm="http://www.aixm.aero/schema/5.1.1"
    xmlns:event="http://www.aixm.aero/schema/5.1.1/event"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
    <import namespace="http://www.aixm.aero/schema/5.1.1" schemaLocation="../../AIXM_DataTypes.xsd"/>
    <simpleType name="CodeAISPublicationBaseType">
        <annotation>
            <documentation>The list of codes in order to identify the type of AIS Publication</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="xsd:string">
                    <enumeration value="AIC">
                        <annotation>
                            <documentation>It is an AIC</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AIP_SUP">
                        <annotation>
                            <documentation>It is an AIP Supplement</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AIP_AMDT">
                        <annotation>
                            <documentation>it is an Amendment</documentation>
                        </annotation>
                    </enumeration>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="xsd:string">
                    <pattern value="OTHER(:(\w|_){1,58})?"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <complexType name="CodeAISPublicationType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type.</documentation>
        </annotation>
        <simpleContent>
            <extension base="event:CodeAISPublicationBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="CodeNOTAMBaseType">
        <annotation>
            <documentation>A code that indicates if the NOTAM is new or a replacement/cancellation of a previous NOTAM</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="xsd:string">
                    <enumeration value="N">
                        <annotation>
                            <documentation>NOTAM New</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="R">
                        <annotation>
                            <documentation>NOTAM that replaces a previously issued NOTAM</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="C">
                        <annotation>
                            <documentation>NOTAM that cancels a previously issued NOTAM</documentation>
                        </annotation>
                    </enumeration>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="xsd:string">
                    <pattern value="OTHER(:(\w|_){1,58})?"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <simpleType name="CodeNOTAMPurposeBaseType">
        <annotation>
            <documentation>A code indicating the intended audience and the reason for which the NOTAM was issued.</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="xsd:string">
                    <enumeration value="NB">
                        <annotation>
                            <documentation>For immediate notification and PIB entry</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="BO">
                        <annotation>
                            <documentation>For PIB entry, concerning flight operations.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="B">
                        <annotation>
                            <documentation>For PIB entry.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="NBO">
                        <annotation>
                            <documentation>For immediate notification and PIB entry, concerning flight operations.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="M">
                        <annotation>
                            <documentation>Miscellaneous - not subject for briefing, but available on request.</documentation>
                        </annotation>
                    </enumeration>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="xsd:string">
                    <pattern value="OTHER(:(\w|_){1,58})?"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <complexType name="CodeNOTAMPurposeType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:CodeNOTAMPurposeBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="CodeNOTAMScopeBaseType">
        <annotation>
            <documentation>A code indicating the significance of the NOTAM for aerodrome or en-route operations</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="xsd:string">
                    <enumeration value="A">
                        <annotation>
                            <documentation>Of interest for aerodrome operations</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="E">
                        <annotation>
                            <documentation>Of interest for en-toute operations</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AE">
                        <annotation>
                            <documentation>Of interest for both en-route and aerodrome</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="W">
                        <annotation>
                            <documentation>Navigation warning</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AW">
                        <annotation>
                            <documentation>Navigation warning, of interest for aerodrome operations</documentation>
                        </annotation>
                    </enumeration>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="xsd:string">
                    <pattern value="OTHER(:(\w|_){1,58})?"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <complexType name="CodeNOTAMScopeType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:CodeNOTAMScopeBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="CodeNOTAMSelectionBaseType">
        <annotation>
            <documentation>A 5-letter code starting with the letter Q that identifies the subject and the condition of the NOTAM. </documentation>
        </annotation>
        <restriction base="xsd:string"/>
    </simpleType>
    <complexType name="CodeNOTAMSelectionType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:CodeNOTAMSelectionBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="CodeNOTAMTrafficBaseType">
        <annotation>
            <documentation>An code indicating the type of traffic based on flight rules concerned by a NOTAM</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="xsd:string">
                    <enumeration value="I">
                        <annotation>
                            <documentation>IFR traffic</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="V">
                        <annotation>
                            <documentation>VFR traffic</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="IV">
                        <annotation>
                            <documentation>Both IFR and VFR traffic</documentation>
                        </annotation>
                    </enumeration>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="xsd:string">
                    <pattern value="OTHER(:(\w|_){1,58})?"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <complexType name="CodeNOTAMTrafficType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:CodeNOTAMTrafficBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <complexType name="CodeNOTAMType">
        <!--No annotation found for this element.-->
        <simpleContent>
            <extension base="event:CodeNOTAMBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="DateTimeNOTAMBaseType">
        <!--No annotation found for this element.-->
        <restriction base="xsd:string"/>
    </simpleType>
    <complexType name="DateTimeNOTAMType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:DateTimeNOTAMBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="TextNOTAMShortBaseType">
        <annotation>
            <documentation>A piece of short text used for NOTAM fields such as location, schedule, etc.</documentation>
        </annotation>
        <restriction base="xsd:string"/>
    </simpleType>
    <complexType name="TextNOTAMShortType">
        <annotation>
            <documentation>A complex data type that enables the provision of a NIL reason for any attribute using this type. </documentation>
        </annotation>
        <simpleContent>
            <extension base="event:TextNOTAMShortBaseType">
                <attribute name="nilReason" type="gml:NilReasonEnumeration">
                    <!--No annotation found for this element.-->
                </attribute>
            </extension>
        </simpleContent>
    </complexType>
</schema>
