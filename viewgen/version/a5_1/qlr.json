    "PointPropertyType" : {
        "one" : {
            "publish" : {
                "geometry" : {
                    "geometrytype" : "Point"
                },
                "form" : [
                    {
                        "field" : "{role}_longitude",
                        "name" : "longitude"
                    },
                    {
                        "field" : "{role}_latitude",
                        "name" : "latitude"
                    },
                    {
                        "field" : "{role}_horizontalaccuracy",
                        "name" : "horizontalaccuracy"
                    }
                ]
            },
            "attribute" : [
                "{alias}.geom::geometry(Point, 4326) AS {role}_geom",
                "{alias}.longitude AS {role}_longitude",
                "{alias}.latitude AS {role}_latitude",
                "{alias}.horizontalAccuracy AS {role}_horizontalaccuracy"
            ], 
            "group" : [
                "{alias}.geom",
                "{alias}.horizontalAccuracy"
            ]
        },
        "many" : {
            "publish" : {
                "geometry" : {
                    "geometrytype" : "Point"
                }
            },
            "attribute" : [
                "{alias}.geom::geometry(MULTIPOINT, 4326) as {role}_geom",
                "{alias}.attr::jsonb as {role}"
            ], 
            "argument" : [
                "ST_Collect(DISTINCT {alias}.geom) AS geom,",
                "jsonb_agg(DISTINCT jsonb_build_object(",
                "  'id', {alias}.id,",
                "  'longitude', {alias}.longitude,",
                "  'latitude', {alias}.latitude,",
                "  'horizontalAccuracy', {alias}.horizontalAccuracy",
                ")) AS attr"
            ],
            "group" : [
                
            ]
        }
    },
